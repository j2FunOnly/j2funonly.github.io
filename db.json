<!DOCTYPE html>
<head>
</head>
<body>
<a href="#" class="letter">a</a>
<a href="#" class="letter">b</a>
<a href="#" class="letter">c</a>
<a href="#" class="letter">d</a>
<ul class="products"></ul>
<script>
  let data = {
    products: []
  };
  
  fetch('http://json.synapsepro.ru/products.json')
    .then(response => response.json())
    .then(json => data = json)
    .catch(err => alert(err));
 
  const renderProducts = products => {
    const list = document.querySelector('ul.products');
    const html = products.reduce((h, product) => {
      return h += `<li>${product.title}</li>`
    }, '');
 
    list.innerHTML = html;
  }
 
  const filterProducts = letter => {
    return data.products.filter(product => {
      return product.title.toLowerCase().startsWith(letter);
    });
  }
 
  const clickHandler = function(e) {
    e.preventDefault();
 
    const filteredProducts = filterProducts(this.textContent.trim());
    renderProducts(filteredProducts);
  }
 
  document.querySelectorAll('a.letter').forEach(el => {
    el.addEventListener('click', clickHandler);
  });
 
</script>
</body>
</html>


